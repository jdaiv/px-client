{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "quotemark": [true, "single", "jsx-double"],
        "curly": false,
        "trailing-comma": false,
        "member-ordering": false,
        "no-var-requires": false,
        "max-classes-per-file": false,
        "forin": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-console": false
    },
    "rulesDirectory": []
}